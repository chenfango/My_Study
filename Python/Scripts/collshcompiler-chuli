        print Log
        print Data
        print FN
        getfile(Log,Data,FN)
        List=['127.0.0.1','user','password','database']
        get_conn(List)
        table_name='comuser'
        sql = 'LOAD DATA LOCAL INFILE \''+ FN+'\' INTO TABLE '+table_name+' FIELDS TERMINATED BY \',\''
        OPS_to_mysql(sql,table_name)
        sql='delete from ' +table_name+' where time < "2018-08-19 23:59:59"'
        OPS_to_mysql(sql,table_name)
